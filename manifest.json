{
    "name": "GitHub Contributions Gradation",
    "version": "0.1",
    "description": "",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": [
            "https://github.com/*"
        ],
        "js":      ["colutil.js", "content.js"],
        "run_at": "document_idle"
    }],
    "permissions" : [
        "tabs",
        "identity",
        "activeTab",
        "https://github.com/*"
    ],
    "manifest_version": 2
}
